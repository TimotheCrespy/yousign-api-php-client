{
    "name": "timothecrespy/yousign-api-php-client",
    "description": "A PHP client library for accessing Yousign REST APIs",
    "keywords": [
        "yousign",
        "api",
        "rest"
    ],
    "license": "MIT",
    "homepage": "https://www.timothecrespy.fr",
    "support": {
        "issues": "https://github.com/timothecrespy/yousign-api-php-client/issues",
        "source": "https://github.com/timothecrespy/yousign-api-php-client"
    },
    "authors": [
        {
            "name": "TimothÃ© Crespy",
            "email": "contact@timothecrespy.fr"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^2.0.2",
        "vlucas/phpdotenv": "^3.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Yousign\\": "src/Yousign/"
        }
    },
    "autoload-dev": {
        "files": [],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "./vendor/bin/phpcs",
            "./vendor/bin/phpunit"
        ]
    }
}